const pin_t MY_LED = D7;
SYSTEM_THREAD(ENABLED);
void setup()
{
	pinMode(MY_LED, OUTPUT);
}


void loop()
{
	
	digitalWrite(MY_LED, HIGH);


	delay(1s);

	
	digitalWrite(MY_LED, LOW);


	delay(1s);
	
	digitalWrite(MY_LED, HIGH);

	
	delay(1s);

    digitalWrite(MY_LED, LOW);


	delay(1s);
	
	digitalWrite(MY_LED, HIGH);

	// Leave it on for one second
	delay(2s);
	
	digitalWrite(MY_LED, LOW);

	// Wait one more second
	delay(1s);
	
	digitalWrite(MY_LED, HIGH);

	// Leave it on for one second
	delay(2s);
	
	digitalWrite(MY_LED, HIGH);

	// Leave it on for one second
	delay(1s);

	// Turn it off
	digitalWrite(MY_LED, LOW);

	// Wait one more second
	delay(1s);
	
	digitalWrite(MY_LED, HIGH);

	// Leave it on for one second
	delay(2s);
	
	digitalWrite(MY_LED, LOW);

	// Wait one more second
	delay(1s);
	
	digitalWrite(MY_LED, HIGH);

	// Leave it on for one second
	delay(1s);

	// Turn it off
	digitalWrite(MY_LED, LOW);

	// Wait one more second
	delay(1s);
	
	digitalWrite(MY_LED, HIGH);

	// Leave it on for one second
	delay(1s);

	// Turn it off
	digitalWrite(MY_LED, LOW);

	// Wait one more second
	delay(1s);
	digitalWrite(MY_LED, HIGH);

	// Leave it on for one second
	delay(2s);
	
	digitalWrite(MY_LED, LOW);

	// Wait one more second
	delay(1s);
	digitalWrite(MY_LED, HIGH);

	// Leave it on for one second
	delay(1s);

	// Turn it off
	digitalWrite(MY_LED, LOW);

	// Wait one more second
	delay(1s);
	digitalWrite(MY_LED, HIGH);

	// Leave it on for one second
	delay(2s);
	
	digitalWrite(MY_LED, LOW);

	// Wait one more second
	delay(1s);
	
	digitalWrite(MY_LED, HIGH);

	// Leave it on for one second
	delay(2s);
	
	digitalWrite(MY_LED, LOW);

	// Wait one more second
	delay(1s);
	digitalWrite(MY_LED, HIGH);

	// Leave it on for one second
	delay(2s);
	
	digitalWrite(MY_LED, LOW);

	// Wait one more second
	delay(1s);
	
	
	// And repeat!
}
